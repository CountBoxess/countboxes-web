{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
